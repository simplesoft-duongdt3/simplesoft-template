<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="AutoCompleteTextViewAppThemeBlue" parent="android:Widget.Holo.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/appthemeblue_list_selector_holo_dark</item>
	    <item name="android:background">@drawable/appthemeblue_edit_text_holo_dark</item>
  </style>

  <style name="ButtonAppThemeBlue" parent="android:Widget.Holo.Button">
	  <item name="android:background">@drawable/appthemeblue_btn_default_holo_dark</item>
  </style>

  <style name="ImageButtonAppThemeBlue" parent="android:Widget.Holo.ImageButton">
	  <item name="android:background">@drawable/appthemeblue_btn_default_holo_dark</item>
  </style>

  <style name="SpinnerAppThemeBlue" parent="android:Widget.Holo.Spinner">
      <item name="android:background">@drawable/appthemeblue_spinner_background_holo_dark</item>
      <item name="android:dropDownSelector">@drawable/appthemeblue_list_selector_holo_dark</item>
  </style>

  <style name="TabAppThemeBlue">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/appthemeblue_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextAppThemeBlue">
	  <item name="android:textColor">#ffffff</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarAppThemeBlue" parent="android:Widget.Holo.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/appthemeblue_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/appthemeblue_progress_indeterminate_horizontal_holo_dark</item>
  </style>

  <style name="SeekBarAppThemeBlue" parent="android:Widget.Holo.SeekBar">
      <item name="android:progressDrawable">@drawable/appthemeblue_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/appthemeblue_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:thumb">@drawable/appthemeblue_scrubber_control_selector_holo_dark</item>
  </style>

  <style name="RatingBarAppThemeBlue" parent="android:Widget.Holo.RatingBar">
      <item name="android:progressDrawable">@drawable/appthemeblue_ratingbar_full_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/appthemeblue_ratingbar_full_holo_dark</item>
  </style>

  <style name="RatingBarBigAppThemeBlue" parent="android:Widget.Holo.RatingBar.Indicator">
