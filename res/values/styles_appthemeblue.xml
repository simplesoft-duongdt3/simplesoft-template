      <item name="android:indeterminateDrawable">@drawable/appthemeblue_ratingbar_small_holo_dark</item>
  </style>

  <style name="ToggleAppThemeBlue" parent="android:Widget.Holo.Button.Toggle">
      <item name="android:background">@drawable/appthemeblue_btn_toggle_holo_dark</item>
  </style>

  <style name="ActionBar.Solid.Appthemeblue" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">@drawable/ab_background_textured_appthemeblue</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_appthemeblue</item>
        <item name="android:backgroundSplit">@drawable/ab_background_textured_appthemeblue</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Appthemeblue</item>
    </style>

    <style name="ActionBar.Transparent.Appthemeblue" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_appthemeblue</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Appthemeblue</item>
    </style>

    <style name="PopupMenu.Appthemeblue" parent="@android:style/Widget.Holo.ListPopupWindow">
